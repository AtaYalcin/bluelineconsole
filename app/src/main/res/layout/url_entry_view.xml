<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_weight="1"
    android:paddingTop="2dp"
    android:paddingBottom="2dp"
    android:paddingStart="10dp"
    android:paddingEnd="10dp"
    android:orientation="vertical"
    android:gravity="center_vertical"
    android:duplicateParentState="true"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/urlDisplayNameOnEntryView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="2dp"
            android:paddingBottom="2dp"
            android:clickable="false"
            android:focusable="false"
            android:textColor="?bluelineconsoleBaseTextColor"
            android:textStyle="bold"
            />
        <Space
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
        />
        <TextView
            android:id="@+id/urlDisabledOnEntryView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="2dp"
            android:paddingBottom="2dp"
            android:clickable="false"
            android:focusable="false"
            android:textColor="?bluelineconsoleBaseTextColor"
            android:textStyle="italic"
            android:text="@string/preferences_url_property_disabled"
            android:visibility="gone"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/preferences_url_column_command_name"
            android:paddingStart="0sp"
            android:paddingEnd="4sp"
            />
        <TextView
            android:id="@+id/urlNameOnEntryView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="2dp"
            android:paddingBottom="2dp"
            android:clickable="false"
            android:focusable="false"
            android:textColor="?bluelineconsoleBaseTextColor"
            />
        <Space
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/urlPresetOnEntryView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="2dp"
            android:paddingBottom="2dp"
            android:clickable="false"
            android:focusable="false"
            android:textColor="?bluelineconsoleBaseTextColor"
            android:textStyle="italic"
            android:text="@string/preferences_url_property_preset"
            android:visibility="gone"
            />
    </LinearLayout>
</LinearLayout>
